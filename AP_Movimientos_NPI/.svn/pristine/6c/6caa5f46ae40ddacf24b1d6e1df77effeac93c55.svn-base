BROKER SCHEMA profuturo.retiros.npi.movimientos.administrar.esql


CREATE COMPUTE MODULE SF_AdministrarMovimientos_ArmaException
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN

		DELETE FIELD InputLocalEnvironment.Destination.SOAP;
		SET Environment.variables.informar.resultado = 'EXCEPTION'; 
		SET Environment.variables.informar.folio = Environment.Request.sr_folio;
		IF Environment.Bitacora = TRUE THEN 
			PROPAGATE TO LABEL 'actualizarBitacora';
		END IF;
		
				
		SET OutputRoot.XMLNSC.nsuca:RespuestaAcreditarMovimientos.nsuca:folio		= Environment.Request.sr_folio;
		SET OutputRoot.XMLNSC.nsuca:RespuestaAcreditarMovimientos.nsuca:estatus		= false;
		SET OutputRoot.XMLNSC.nsuca:RespuestaAcreditarMovimientos.nsuca:mensaje		= 'Error servicio';
				

		RETURN TRUE;
	END;

END MODULE;
